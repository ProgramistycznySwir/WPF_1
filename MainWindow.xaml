<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Text="{Binding TextBox_Text, Mode = TwoWay}" HorizontalAlignment="Center" Height="23" Margin="0" TextWrapping="Wrap" VerticalAlignment="Center" Width="120"/>
        <Button Name="Button_In" Content="Wrzuć" HorizontalAlignment="Center" Margin="-100,100,0,0" VerticalAlignment="Center" Width="75" RenderTransformOrigin="0.5,0.5" HorizontalContentAlignment="Center" Click="Button_Click"/>
        <Button Name="Button_Out" Content="Wyjmij" HorizontalAlignment="Center" Margin="100,100,0,0" VerticalAlignment="Center" Width="75" Click="Button_Click"/>
        <Label Content="{Binding Label_Text, Mode = TwoWay}" HorizontalAlignment="Center" Margin="0,-100,0,0" VerticalAlignment="Center"/>

    </Grid>
</Window>
